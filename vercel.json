{
  "version": 2,
  "name": "harsh-gpt",
  "cleanUrls": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/public/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/public/sw.js"
    },
    {
      "src": "/icon-192.png",
      "dest": "/public/icon-192.png"
    },
    {
      "src": "/icon-512.png",
      "dest": "/public/icon-512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}